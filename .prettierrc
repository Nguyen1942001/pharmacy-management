{
  "trailingComma": "es5",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "printWidth": 120,
  "proseWrap": "always",
  "quoteProps": "as-needed",
  "bracketSpacing": true,
  "bracketSameLine": false,
  "arrowParens": "always",
  "singleAttributePerLine": false,
  "jsxBracketSameLine": false,
  "jsxSingleQuote": true,
  "jsxBracketSpacing": true,
  "jsxPrintWidth": 120,
  "endOfLine": "auto",
  "plugins": ["prettier-plugin-tailwindcss"]
}
